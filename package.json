{
  "name": "Issues-Analytics",
  "version": "0.1.0",
  "description": "github issues analytics",
  "main": "index.js",
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-scripts test --env=node --coverage",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Intern2020OJT/CT_OBJ.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Intern2020OJT/CT_OBJ/issues"
  },
  "homepage": "https://intern2020ojt.github.io/CT_OBJ",
  "dependencies": {
    "@antv/data-set": "^0.10.2",
    "antd": "3.25.3",
    "axios": "0.19.0",
    "bizcharts": "3.5.6",
    "history": "4.9.0",
    "moment": "2.24.0",
    "prop-types": "15.7.2",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-intl": "3.1.8",
    "react-router-dom": "5.0.1",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "babel-plugin-import": "^1.12.0",
    "customize-cra": "^0.5.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint-config-airbnb": "^18.0.1",
    "gh-pages": "^2.1.1",
    "husky": "^3.0.3",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.2.1",
    "prettier": "^1.18.2",
    "react-app-rewired": "^2.1.3",
    "react-scripts": "^3.1.1",
    "react-styleguidist": "^9.1.14",
    "react-test-renderer": "^16.9.0",
    "source-map-explorer": "^2.0.1",
    "webpack-ant-icon-loader": "^1.0.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "eslint --fix ./src --ext .jsx",
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
